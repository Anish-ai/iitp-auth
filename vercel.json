{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "SMTP_HOST": "@smtp_host",
    "SMTP_PORT": "@smtp_port",
    "SMTP_USER": "@smtp_user",
    "SMTP_PASS": "@smtp_pass",
    "JWT_SECRET": "@jwt_secret",
    "FROM_EMAIL": "@from_email",
    "FROM_NAME": "@from_name"
  }
}
